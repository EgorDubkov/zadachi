using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Экипаж
    {
        public string командир;
        public string штурман;
    }

    class Самолет
    {
        public long nomer;
        public string type;
        public double resurs;



        public double get
        {
            set { resurs = value; }
            get { return resurs; }
        }


        public double set
        {
            set
            {
                if (value >= 200)
                    Console.Write(nomer);
            }
        }
    }

    class Авиарейс
    {
        private Самолет сам;
        private Экипаж экип;

        public Авиарейс(Самолет сам, Экипаж экип)
        {
            this.сам = сам;
            this.экип = экип;
        }
        public void show()
        {
            Console.WriteLine("{0}, {1}, {2}, {3}, {4}", сам.nomer, сам.resurs, сам.type, экип.командир, экип.штурман);
        }
        class Program
        {
            static void Main(string[] args)
            {
                Самолет с;
                Экипаж э;

                с = new Самолет();
                с.nomer = 123123;
                с.resurs = 123;
                с.type = "type";

                э = new Экипаж();
                э.командир = "Иван";
                э.штурман = "Василий";

                Console.Read();
            }

        }
    }
}
